{
    "configurations": [
        {
            "name": "Mac",
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${default}",
                "${workspaceFolder}",
                "${workspaceFolder}/src",
                "/usr/local/opt/qt",
                "/usr/local/opt/qt/lib/QtCore.framework/Versions/A/Headers",
                "${workspaceFolder}/deps/socket.io/lib/rapidjson/include"
            ]
        },
        {
            "name": "Win32",
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.18362.0",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "includePath": [
                "${default}",
                "${workspaceFolder}",
                "${env:MINGW64_ROOT}/include/qt6",
                "${env:MINGW64_ROOT}/include/qt6/QtCore",
                "${env:MINGW64_ROOT}/include/qt6/QtWidgets",
                "${env:MINGW64_ROOT}/include/qt6/QtGui",
                "${workspaceFolder}/deps/socket.io/lib/rapidjson/include"
            ]
        }
    ],
    "version": 4
}